<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="OrganicProductBase">
    <defName>SquiddleInk</defName>
    <label>squiddle ink</label>
    <description>Ink from a Squiddle</description>
    <graphicData>
      <texPath>Things/Item/squiddle_ink</texPath>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>3.1</MarketValue>
      <Mass>0.045</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.01</Nutrition>
      <FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
    </statBases>
    <ingestible>
      <preferability>DesperateOnly</preferability>
      <foodType>AnimalProduct, Fluid</foodType>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
  </ThingDef>
  <ThingDef ParentName="EggFertBase">
    <defName>EggSquiddleFertilized</defName>
    <label>squiddle egg (fert.)</label>
    <description>A fertilized squiddle egg. If all goes well, it should hatch into a baby squiddle. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggOval</texPath>
      <color>(225,225,225)</color>
    </graphicData>
    <statBases>
      <MarketValue>25.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>3.5</hatcherDaystoHatch>
        <hatcherPawn>Squiddle</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  <ThingDef ParentName="LeatherBase">
    <defName>Leather_Squiddle</defName>
    <label>squiddle leather</label>
    <graphicData>
      <color>(20,20,20)</color>
    </graphicData>
    <stuffProps>
      <color>(20,20,20)</color>
      <commonality>0.075</commonality>
    </stuffProps>
  </ThingDef>
</Defs>
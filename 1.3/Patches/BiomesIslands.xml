        <wildBiomes>
          <BiomesIslands_TropicalIsland>1</BiomesIslands_TropicalIsland>
          <BiomesIslands_Atoll>1</BiomesIslands_Atoll>
        </wildBiomes>
      </value>
    </match>
  </Operation>
</Patch>

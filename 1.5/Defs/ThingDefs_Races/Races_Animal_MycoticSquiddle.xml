<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BaseSquiddle">
    <defName>MycoticSquiddle</defName>
    <label>Mycotic Squiddle</label>
    <description>The Mycotic Squiddle is a violet squiddle that lives in Mycotic Jungles. They eat fungi. Like all Squiddles they are "milked" for their ink.</description>
    <statBases>
      <MoveSpeed>1.75</MoveSpeed>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>50</ComfyTemperatureMax>
      <MarketValue>200</MarketValue>
    </statBases>
    <race>
      <useMeatFrom>Squiddle</useMeatFrom>
      <baseBodySize>0.8</baseBodySize>
      <baseHealthScale>2.7</baseHealthScale>
      <foodType>OmnivoreRoughAnimal</foodType>
    </race>
    <comps>
      <li Class="CompProperties_Milkable">
        <milkDef>SquiddleInk</milkDef>
        <milkIntervalDays>3</milkIntervalDays>
        <milkAmount>17</milkAmount>
        <milkFemaleOnly>false</milkFemaleOnly>
      </li>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggMycoticSquiddleFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>20</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1~3</eggCountRange>
      </li>
    </comps>
    <tradeTags>
      <li>AnimalCommon</li>
    </tradeTags>
  </ThingDef>
</Defs>

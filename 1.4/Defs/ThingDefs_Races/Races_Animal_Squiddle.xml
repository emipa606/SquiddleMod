<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BaseSquiddle">
    <defName>Squiddle</defName>
    <label>Common Squiddle</label>
    <description>The Common Squiddle is a redish squiddle that lives in Temperate Swamps. They were the orginal squiddle which is a GMO squid. Like all Squiddles they are "milked" for their ink.</description>
    <statBases>
      <MoveSpeed>1.75</MoveSpeed>
      <ComfyTemperatureMin>-5</ComfyTemperatureMin>
      <MarketValue>200</MarketValue>
    </statBases>
    <race>
      <meatLabel>squiddle meat</meatLabel>
      <baseBodySize>0.5</baseBodySize>
      <baseHealthScale>2.0</baseHealthScale>
      <wildBiomes>
        <TemperateSwamp>1</TemperateSwamp>
      </wildBiomes>
    </race>
    <comps>
      <li Class="CompProperties_Milkable">
        <milkDef>SquiddleInk</milkDef>
        <milkIntervalDays>2</milkIntervalDays>
        <milkAmount>10</milkAmount>
        <milkFemaleOnly>false</milkFemaleOnly>
      </li>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggSquiddleFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>20</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1~3</eggCountRange>
      </li>
    </comps>
    <tradeTags>
      <li>AnimalCommon</li>
    </tradeTags>
  </ThingDef>
</Defs>
